{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Build C - main.c",
        "type": "shell",
        "command": "C:/msys64/mingw64/bin/gcc.exe",
        "args": [
          "src/main.c",
          "-o",
          "bin/main.exe",
          "-g"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "problemMatcher": ["$gcc"],
        "detail": "Compile dengan MSYS2 gcc"
      },
      {
        "label": "Run C Program",
        "type": "shell",
        "command": ".\\bin\\main.exe",
        "dependsOn": "Build C - main.c",
        "group": "test",
        "problemMatcher": []
      }
    ]
  }
  